{
  "name": "webpack-ang",
  "version": "1.0.0",
  "description": "webpack angularjs打包基础配置",
  "main": "webpack.config.js",
  "scripts": {
    "start": "webpack-dev-server --inline --config ./webpack.dev.config.js",
    "watch": "rimraf dist && webpack --watch --config ./webpack.build.config.js",
    "build": "rimraf dist && webpack --config ./webpack.build.config.js",
    "profile": "webpack --config ./webpack.build.config.js --json --profile > stats.json"
  },
  "eslintConfig": {
    "extends": "enough",
    "env": {
      "browser": true,
      "node": true
    }
  },
  "keywords": [
    "webpack"
  ],
  "author": "xyl",
  "license": "ISC",
  "devDependencies": {
    "animate.css": "^3.5.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "compass-mixins": "^0.12.10",
    "cors": "^2.8.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "eslint": "^3.19.0",
    "eslint-config-enough": "^0.2.6",
    "eslint-loader": "^1.7.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.0",
    "fs": "^0.0.1-security",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "html-withimg-loader": "^0.1.16",
    "http-proxy": "^1.16.2",
    "jshint": "^2.9.4",
    "jshint-loader": "^0.8.3",
    "json-loader": "^0.5.4",
    "less-middleware": "^2.2.0",
    "node-sass": "^4.5.0",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "react": "^15.4.2",
    "reload": "^1.1.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.5.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^1.16.5"
  },
  "dependencies": {
    "connect-flash": "^0.1.1",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "log4js": "^1.1.1",
    "mysql": "^2.13.0",
    "serve-favicon": "^2.4.2"
  }
}
